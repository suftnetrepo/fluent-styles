{
  "name": "fluent-styles",
  "version": "1.49.0",
  "description": "Develop different styled versions of UI components.",
  "main": "lib/commonjs/index.ts",
  "module": "lib/module/index.ts",
  "types": "lib/typescript/index.d.ts",
  "exports": {
    ".": {
      "types": "./lib/typescript/index.d.ts",
      "require": "./lib/commonjs/index.js",
      "import": "./lib/module/index.js"
    }
  },
  "react-native": "src/index",
  "source": "src/index",
  "repository": {
    "type": "git",
    "url": "https://github.com/suftnetrepo/fluent-styles.git"
  },
  "author": "Abel Aghorighor <52415319+aaghorighor@users.noreply.github.com>",
  "license": "MIT",
  "keywords": [
    "react-native",
    "styles",
    "variants",
    "styling",
    "ios",
    "android"
  ],
  "bugs": {
    "url": "https://github.com/suftnetrepo/fluent-styles/issues"
  },
  "homepage": "https://github.com/suftnetrepo/fluent-styles#readme",
  "dependencies": {
    "@babel/core": "^7.29.0",
    "@gorhom/bottom-sheet": "^5.2.8",
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-native-community/datetimepicker": "^8.6.0",
    "@react-native-community/slider": "^5.1.2",
    "babel-loader": "^8.4.1",
    "react-native-dropdown-picker": "^5.4.6",
    "react-native-gesture-handler": "^2.30.0",
    "react-native-reanimated": "~3.16.0",
    "react-native-safe-area-context": "^5.6.2",
    "react-native-svg": "^15.15.3",
    "react-native-vector-icons": "^9.2.0"
  },
  "files": [
    "src",
    "lib",
    "!expo",
    "!node_modules"
  ],
  "scripts": {
    "test": "jest",
    "build": "bob build",
    "prepack": "bob build",
    "release": "release-it",
    "publish": "npm publish --access public",
    "lint": "eslint \"**/*.{js,ts,tsx}\""
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-native": ">=0.60.0"
  },
  "devDependencies": {
    "@babel/plugin-transform-class-properties": "^7.24.7",
    "@babel/plugin-transform-private-methods": "^7.24.7",
    "@babel/plugin-transform-private-property-in-object": "^7.24.7",
    "@babel/preset-env": "^7.24.7",
    "@react-native-community/bob": "^0.17.1",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^12.5.1",
    "@types/react": "^18.x",
    "@types/react-native": "^0.73.x",
    "jest": "^29.7.0",
    "metro-react-native-babel-preset": "^0.77.0",
    "react-test-renderer": "^18.3.1",
    "release-it": "^14.2.2"
  },
  "@react-native-community/bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      "commonjs",
      "module",
      "typescript"
    ]
  },
  "release-it": {
    "git": {
      "commitMessage": "chore: release ${version}",
      "tagName": "v${version}"
    },
    "npm": {
      "publish": true
    },
    "github": {
      "release": true
    }
  },
  "eslintIgnore": [
    "node_modules/",
    "lib/"
  ],
  "private": true,
  "workspaces": [
    "storyBook/demo"
  ]
}
